<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<Dialog id="pdfDialog" title="{pdfModel>/alt}" stretch="true" verticalScrolling="false">
 
    <PDFViewer source="{pdfModel>/src}" 
      showDownloadButton="false"
      isTrustedSource="true" 
      height="600px"
    
      />

  <beginButton>
    <Button
    text="{i18n>Chiudi}"
    press="onCloseDialog"
/>
  </beginButton>
</Dialog>
</c:FragmentDefinition>