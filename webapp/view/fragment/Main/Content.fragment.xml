<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <VBox
        direction="Row"
        class="sapUiLargeMarginTop"
        justifyContent="SpaceAround"
        wrap="Wrap"
    >
        <GenericTile
            class="sapUiResponsiveMargin tileLayout custTile"
            header="{i18n>title}"
            press="navToQualifica"
            headerImage="sap-icon://doc-attachment"
        >
            <TileContent >

                <!-- <ImageContent                  
                    class="content"
                    src="sap-icon://doc-attachment"
                /> -->
            </TileContent>
        </GenericTile>
        <GenericTile
            class="sapUiResponsiveMargin tileLayout custTile"
            header="{i18n>titleRdo}"
            press="navToRdo"
            headerImage="sap-icon://add-activity"
        >
            <TileContent>
                <!-- <ImageContent
                    class="icon"
                    src="sap-icon://add-activity"
                /> -->
            </TileContent>
        </GenericTile>
        <GenericTile
            class="sapUiResponsiveMargin tileLayout custTile"
            header="{i18n>titleOda}"
            press="navToOda"
            headerImage="sap-icon://activity-2"
        >
            <TileContent>
                <!-- <ImageContent
                    class="icon"
                    src="sap-icon://activity-2"
                /> -->
            </TileContent>
        </GenericTile>
        <GenericTile
        visible="false"
            class="sapUiResponsiveMargin tileLayout custTile"
            header="{i18n>titleFatt}"
            press="navToPayment"
            headerImage="sap-icon://payment-approval"
            mode="IconMode"
        >
            <TileContent>
                <!-- <ImageContent
                    class="icon"
                    src="sap-icon://payment-approval"
                /> -->
            </TileContent>
        </GenericTile>
    </VBox>
</c:FragmentDefinition>
