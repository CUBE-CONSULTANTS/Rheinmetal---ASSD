<core:FragmentDefinition
xmlns="sap.m"
xmlns:core="sap.ui.core"
>
<TableSelectDialog
  multiSelect="false"
  contentHeight="50%"
  contentWidth="30%"
  title="{i18n>selezionaDescr}"
  search=".handleSearchDescr"
  confirm="onConfirmFilter"
  cancel=".handleClose"
  items="{RicercaFornModel>/filterbar/Descrizione}"
>
  <columns>
    <Column>
      <header>
        <Text text="{i18n>Descrizione}" />
      </header>
    </Column>
  </columns>
  <ColumnListItem>
    <cells>
      <Text text="{RicercaFornModel>NAME1}" />

    </cells>
  </ColumnListItem>
</TableSelectDialog>
</core:FragmentDefinition>