<core:FragmentDefinition
xmlns="sap.m"
xmlns:core="sap.ui.core"
>
<TableSelectDialog
multiSelect="false"
  contentHeight="50%"
  contentWidth="30%"
  title="{i18n>selezionaPaese}"
  search=".handleSearchPaese"
  confirm="onConfirmFilter"
  cancel=".handleClose"
  items="{RicercaFornModel>/filterbar/Paese}"
>
  <columns>
    <Column>
      <header>
        <Text text="{i18n>Paese}" />
      </header>
    </Column>
    
  </columns>
  <ColumnListItem>
    <cells>
      <Text text="{RicercaFornModel>COUNTRY}" />

    </cells>
  </ColumnListItem>
</TableSelectDialog>
</core:FragmentDefinition>