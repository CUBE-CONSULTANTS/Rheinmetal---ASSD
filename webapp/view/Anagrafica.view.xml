<mvc:View controllerName="rheinmetalassd.controller.Anagrafica"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    height="100%"
    displayBlock="true">
    <f:DynamicPage showFooter="true"
    preserveHeaderStateOnScroll="true"
        >
        <f:header>
            <core:Fragment fragmentName="rheinmetalassd.view.fragment.QualificaForn.anagrafica.Header"
            type="XML" />           
        </f:header>
        
        <f:content>
            <core:Fragment fragmentName="rheinmetalassd.view.fragment.QualificaForn.anagrafica.form"
            type="XML" />
        </f:content>
        <f:footer>
            <core:Fragment fragmentName="rheinmetalassd.view.fragment.QualificaForn.anagrafica.Footer"
            type="XML" />
        </f:footer>
    </f:DynamicPage>
</mvc:View>